- content_for :title_header do
	= form_tag admin_urls_path, :method => :get, :style => "float: right; position: relative; top: -45px;" do
		= text_field_tag :search, params[:search], :placeholder => "Search URLs", :style => "width: 125px"


%table.table.table-striped.table-bordered
	%thead
		%tr
			%th Long URL
			%th Short URL
			%th Clicks
			%th
	%tbody
		= render @urls